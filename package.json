{
  "name": "dovedit",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "wrangler pages dev ./dist",
    "db:migrate": "drizzle-kit generate && wrangler d1 migrations apply dovedit-db --local",
    "db:migrate:remote": "drizzle-kit generate && wrangler d1 migrations apply dovedit-db --remote",
    "db:studio": "drizzle-kit studio",
    "bindings:typegen": "wrangler types",
    "astro": "astro",
    "wrangler": "wrangler"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.0.1",
    "@astrojs/react": "^4.0.0",
    "@astrojs/tailwind": "^5.1.3",
    "@fontsource/geist-sans": "^5.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^5.0.3",
    "drizzle-orm": "^0.38.2",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "better-sqlite3": "^11.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.30.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2"
  }
}
